{
  "name": "appkr/rest",
  "description": "General-purpose Restful API Service",
  "keywords": [
    "restful",
    "api",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Appkr",
      "email": "juwonkim@me.com"
    }
  ],
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:appkr/socialite.git"
    }
  ],

  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.1.*",
    "appkr/socialite": "dev-master",
    "appkr/api": "0.1.*",
    "barryvdh/laravel-cors": "0.7.x",
    "tymon/jwt-auth": "0.5.*",
    "laravelcollective/html": "~5.0",
    "bican/roles": "2.*",
    "laracasts/flash": "~1.3",
    "intervention/image": "2.3.*",
    "watson/rememberable": "1.*",
    "erusev/parsedown-extra": "0.7.*",
    "maknz/slack": "1.*",
    "graham-campbell/binput": "~3.2",
    "graham-campbell/throttle": "~4.1",
    "jenssegers/optimus": "^0.1.3"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1",
    "spatie/laravel-tail": "~1.1",
    "laravel/homestead": "~2.1",
    "php-vcr/php-vcr": "~1.2",
    "php-vcr/phpunit-testlistener-vcr": "~1.1",
    "guzzlehttp/guzzle": "^6.0@dev",
    "barryvdh/laravel-ide-helper": "^2.0"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "pre-update-cmd": [
      "php artisan clear-compiled"
    ],
    "post-update-cmd": [
      "php artisan optimize"
    ],
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
